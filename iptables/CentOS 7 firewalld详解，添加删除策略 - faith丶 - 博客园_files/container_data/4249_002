    function isIEBrowserVersion9_31277() {
        return (isIEBrowser_31277 && (browserVersion_31277 == 9)) ? true : false;
    }

    function isEdgeSupported_31277() {
        if(isIEBrowserVersion9_31277()) {
            return "y";           //hardcoding IE9 edge support.
        }
        if(hasTransform_31277) {
            if(requiresSVG_31277&&!hasSVG_31277)
                return "f";
            return "y";
        }
        return "f";
    }

    function isCanvasSupported_31277(){
      var elem = document.createElement('canvas');
      return !!(elem.getContext && elem.getContext('2d'));
    }

    function isHTML5FeaturesSupported_31277() {
         return (isCanvasSupported_31277()) ? "y" : "f";
    }

    var requiresSVG_31277=false;
    //edge detection code end

