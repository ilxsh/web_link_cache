                '         data-ad-client="ca-pub-8616102841876629"' +
                '         data-ad-slot="4937183096"><\/ins>' +
                '    <script>' +
                '         (adsbygoogle = window.adsbygoogle || []).push({});' +
                '    <\/script>' +
                '<\/div>';
    }
    else if (ad_id == 'content_bottom') {
