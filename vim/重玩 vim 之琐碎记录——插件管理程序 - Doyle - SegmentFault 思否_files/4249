    function isIEBrowserVersion9_400756() {
        return (isIEBrowser_400756 && (browserVersion_400756 == 9)) ? true : false;
    }

    function isEdgeSupported_400756() {
        if(isIEBrowserVersion9_400756()) {
            return "y";           //hardcoding IE9 edge support.
        }
        if(hasTransform_400756) {
            if(requiresSVG_400756&&!hasSVG_400756)
                return "f";
            return "y";
        }
        return "f";
    }

    function isCanvasSupported_400756(){
      var elem = document.createElement('canvas');
      return !!(elem.getContext && elem.getContext('2d'));
    }

    function isHTML5FeaturesSupported_400756() {
         return (isCanvasSupported_400756()) ? "y" : "f";
    }

    var requiresSVG_400756=false;
