     function getInternetExplorerVersion_400756() {
         // Returns the version of Internet Explorer or a -1
         // (indicating the use of another browser).

             var rv = -1; // Return value assumes failure.
             if (navigator.appName == 'Microsoft Internet Explorer') {
                 isIEBrowser_400756=true;
                 var ua = navigator.userAgent;
                 var re  = new RegExp("MSIE ([0-9]{1,}[\.0-9]{0,})");

                 if (re.exec(ua) != null)
                     rv = parseFloat( RegExp.$1 );
             }

             return rv;
         }

      //returns true if ie version is less than 9, say ie6, ie7, ie8
         // -1 for non IE browsers.
         function isIEBrowserWithVersionLessThan9_400756 () {

             browserVersion_400756 = getInternetExplorerVersion_400756();  //-1 for non IE browsers
             if((browserVersion_400756 != -1) && (browserVersion_400756 < 9)) {
                 return true;

             }
             return false;
         }

    //code to detect Edge Features, courtesy  (http://dl.dropboxusercontent.com/u/13483458/test-edge.html)
    var testEle_400756=document.createElement("div_400756");
    function isSupported_400756(a){

        var d=testEle_400756.style,e;
        for(i=0;i<a.length;i++)
            if(e=a[i],d[e]!==void 0)
                return!0;
        return!1
    }

    function supportsRGBA_400756(){

        testEle_400756.cssText="background-color:rgba(150,255,150,.5)";
        if((""+testEle_400756.style.backgroundColor).indexOf("rgba")==0)
            return!0;
        return!1
    }

    var hasTransform_400756=isSupported_400756([
        "transformProperty",
