    function isIEBrowserVersion9_630987() {
        return (isIEBrowser_630987 && (browserVersion_630987 == 9)) ? true : false;
    }

    function isEdgeSupported_630987() {
        if(isIEBrowserVersion9_630987()) {
            return "y";           //hardcoding IE9 edge support.
        }
        if(hasTransform_630987) {
            if(requiresSVG_630987&&!hasSVG_630987)
                return "f";
            return "y";
        }
        return "f";
    }

    function isCanvasSupported_630987(){
      var elem = document.createElement('canvas');
      return !!(elem.getContext && elem.getContext('2d'));
    }

    function isHTML5FeaturesSupported_630987() {
         return (isCanvasSupported_630987()) ? "y" : "f";
    }

